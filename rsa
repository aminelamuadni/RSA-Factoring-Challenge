#!/usr/bin/env python3

import sys
import random

def gcd(a, b):
    while b:
        a, b = b, a % b
    return abs(a)

def pollards_rho(n):
    if n % 2 == 0:
        return 2
    x = random.randint(1, n-1)
    y = x
    c = random.randint(1, n-1)
    d = 1
    while d == 1:
        x = (x*x + c) % n
        y = (y*y + c) % n
        y = (y*y + c) % n
        d = gcd(x-y, n)
    return d

def factorize_rsa(n):
    factor = pollards_rho(n)
    return factor, n // factor

def main(filename):
    with open(filename, 'r') as file:
        n = int(file.readline().strip())
        p, q = factorize_rsa(n)
        print(f"{n}={max(p, q)}*{min(p, q)}")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: rsa <file>")
        sys.exit(1)
    main(sys.argv[1])
