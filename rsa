#!/usr/bin/python3

import sys
import math

def factorize_rsa(n):
    if n % 2 == 0:
        return (n // 2, 2)

    for i in range(3, int(math.sqrt(n)) + 1, 2):
        if n % i == 0:
            return (n // i, i)
    return (n, 1)  # n is prime

def main():
    if len(sys.argv) < 2:
        print("Usage: rsa <file>")
        sys.exit(1)

    with open(sys.argv[1], 'r') as f:
        number = int(f.readline().strip())
        p, q = factorize_rsa(number)
        print("{}={}*{}".format(number, p, q))

if __name__ == "__main__":
    main()
