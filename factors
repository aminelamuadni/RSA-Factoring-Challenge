#!/usr/bin/env python3

import sys

def factorize(n):
    for p in range(2, n):
        if n % p == 0:
            return p, n // p
    return n, 1

def main():
    if len(sys.argv) != 2:
        print(f'Usage: {sys.argv[0]} <file>')
        sys.exit(1)

    with open(sys.argv[1], 'r') as file:
        for line in file:
            n = int(line.strip())
            p, q = factorize(n)

            if p > q:
                p, q = q, p

            print(f"{n}={p}*{q}")

if __name__ == "__main__":
    main()
